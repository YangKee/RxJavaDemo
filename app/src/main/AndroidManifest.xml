<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.yangyee.com.rxjava">
    <!-- 8.0安装未知应用来源 -->
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name="example.yangyee.com.rxjava.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="example.yangyee.com.rxjava.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="example.yangyee.com.rxjava.Example_1"></activity>
        <activity android:name="example.yangyee.com.rxjava.Example_4_Flowable"></activity>
    </application>

</manifest>